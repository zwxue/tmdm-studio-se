			<xsd:element maxOccurs="1" minOccurs="1" name="contentKeywords" nillable="true" type="xsd:string"/>
			<xsd:element maxOccurs="1" minOccurs="1" name="keysKeywords" nillable="true" type="xsd:string"/>
