	<!-- *****************************************************
		P U T    I T E M		
		*******************************************************-->
	<xsd:complexType name="WSPutItem">
		<xsd:annotation>
			<xsd:documentation>
				Puts an item in the xml storage.
			</xsd:documentation>
		</xsd:annotation>
