			<xsd:element maxOccurs="1" minOccurs="1" name="wsViewPK" nillable="false" type="tns:WSViewPK"/>
		</xsd:sequence>		
	</xsd:complexType>
	<xsd:element name="WSExistsView" type="tns:WSExistsView"/>

