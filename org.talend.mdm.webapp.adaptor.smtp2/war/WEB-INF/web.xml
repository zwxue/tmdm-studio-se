<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>smtp2Servlet</servlet-name>
    <servlet-class>org.talend.mdm.webapp.general.gwt.ProxyGWTServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>smtp2Servlet</servlet-name>
    <url-pattern>/smtp/Smtp2Service</url-pattern>
  </servlet-mapping>
  
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>smtp2.html</welcome-file>
  </welcome-file-list>

</web-app>
